<style type="text/css">
    

    .carousel {
        margin-bottom: 0;
        padding: 0 40px 30px 40px;
    }

    .carousel-control {
        left: -12px;
        height: 40px;
        width: 40px;
        background: none repeat scroll 0 0 #222222;
        border: 4px solid #FFFFFF;
        border-radius: 23px 23px 23px 23px;
        margin-top: 90px;
    }

        .carousel-control.right {
            right: -12px;
        }

    .carousel-indicators {
        right: 50%;
        top: auto;
        bottom: -10px;
        margin-right: -19px;
    }
        /* The colour of the indicators */
        .carousel-indicators li {
            background: #cecece;
        }

        .carousel-indicators .active {
            background: #428bca;
        }
</style>
<link rel="stylesheet" href="~/Content/Manh/assets/css/abc.min.css">
<script src="~/Content/Manh/assets/js/abcd.min.js"></script>
<script src="~/Content/Manh/assets/js/abc.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('#Carousel').carousel({
            interval: 5000
        }
        )
    }
    );

</script>
