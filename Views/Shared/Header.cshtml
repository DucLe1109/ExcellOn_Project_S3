<script>
    $(function () {
        var path = window.location.href;
        $('.collapse li a ').each(function () {
            if (this.href === path) {
                $('.collapse li a ').parent().removeClass("active");
                $(this).parent().addClass('active');
            }
        });
    })
</script>
<script type="text/javascript">

    function _Profile() {
        $(location).attr("href", "");
    }
    function _Logout() {
        $(location).attr("href", "/Customer/Logout");
    }

</script>

