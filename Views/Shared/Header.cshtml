            </h1>
            <!-- if logo is image enable this
            <a class="navbar-brand" href="#index.html">
                <img src="image-path" alt="Your logo" title="Your logo" style="height:35px;" />
            </a> -->
            <button class="navbar-toggler  collapsed bg-gradient" type="button" data-toggle="collapse"
                    data-target="#navbarTogglerDemo02" aria-controls="navbarTogglerDemo02" aria-expanded="false"
                    aria-label="Toggle navigation">
                <span class="navbar-toggler-icon fa icon-expand fa-bars"></span>
                <span class="navbar-toggler-icon fa icon-close fa-times"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarTogglerDemo02">
                <ul class="navbar-nav ml-lg-auto">
                    <li class="nav-item active">
                        <a class="nav-link" href="@Url.Action("Index","Home")">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">About</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("service","Services")">Services</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("Index","Contact")">Contact Us</a>
                    </li>

                    @{
                        var cus = (Customer)Session["CustomerName"];
                    }
                    @* <ul class="navbar-nav align-items-center  ml-auto ml-md-0 ">*@
                    @if (cus == null)
                    {
                        <li class="nav-item">
                            <a class="nav-link" href="@Url.Action("Login", "Customer")">Log In</a>
                        </li>
                    }
                    else
                    {
                        <li class="nav-item dropdown">
                            <a class="nav-link pr-0" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <div class="media align-items-center" style="margin-top: -7px; ">
                                    <span class="avatar avatar-sm rounded-circle">
                                        <img alt="Image placeholder" src="@cus.Customer_Avatar">
                                    </span>
                                    <div class="media-body  ml-2  d-none d-lg-block">
                                        <span class="mb-0 text-sm  font-weight-bold">@cus.Customer_Name</span>
                                    </div>
                                </div>
                            </a>
                            <div class="dropdown-menu dropdown-menu-right ">
                                <div class="dropdown-header noti-title">
                                    <h6 class="text-overflow m-0">Welcome!</h6>
                                </div>
                                <a class="dropdown-item" onclick="return _Profile()">
                                    <i class="ni ni-single-02"></i>
                                    <span>My profile</span>
                                </a>

                                <div class="dropdown-divider"></div>
                                <a onclick="return _Logout()" class="dropdown-item">
                                    <i class="ni ni-user-run"></i>
                                    <span>Logout</span>
                                </a>
                            </div>
                        </li>
                    }
                    <!-- search button -->

                    <div class="search-right" @*style="margin: -5px;"*@>
                        <a href="#search" class="btn" title="search">
                            <span class="fa fa-search"
                                  aria-hidden="true"></span>
                        </a>
                        <div id="search" class="pop-overlay">
                            <div class="s002">
                                <form>
                                    <fieldset>
                                        <legend>SEARCH SERVICE</legend>
                                    </fieldset>
                                    <div class="inner-form">
                                        <div class="input-field second-wrap">
                                            <div class="icon-wrap">
                                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                                                    <path d="M17 12h-5v5h5v-5zM16 1v2H8V1H6v2H5c-1.11 0-1.99.9-1.99 2L3 19c0 1.1.89 2 2 2h14c1.1 0 2-.9 2-2V5c0-1.1-.9-2-2-2h-1V1h-2zm3 18H5V8h14v11z"></path>
                                                </svg>
                                            </div>
                                            <input class="datepicker" id="depart" type="text" placeholder="Date-Start" />
                                        </div>
                                        <div class="input-field third-wrap">
                                            <div class="icon-wrap">
                                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                                                    <path d="M17 12h-5v5h5v-5zM16 1v2H8V1H6v2H5c-1.11 0-1.99.9-1.99 2L3 19c0 1.1.89 2 2 2h14c1.1 0 2-.9 2-2V5c0-1.1-.9-2-2-2h-1V1h-2zm3 18H5V8h14v11z"></path>
                                                </svg>
                                            </div>
                                            <input class="datepicker" id="return" type="text" placeholder="Date-End" />
                                        </div>
                                        <div class="input-field fouth-wrap" style="text-align: center;">
                                            <div class="icon-wrap">
                                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                                                    <path d="M12 12c2.21 0 4-1.79 4-4s-1.79-4-4-4-4 1.79-4 4 1.79 4 4 4zm0 2c-2.67 0-8 1.34-8 4v2h16v-2c0-2.66-5.33-4-8-4z"></path>
                                                </svg>
                                            </div>
                                            <select data-trigger="" name="choices-single-defaul" class="center">
                                                <option placeholder="">Service</option>
                                                <option>In-bound</option>
                                                <option>Out-bound</option>
                                                <option>Tele Marketing</option>
                                            </select>
                                        </div>
                                        <div class="input-field fifth-wrap">
                                            <button class="btn-search" type="button">SEARCH</button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                            <a class="close" href="#close">×</a>
                            <!-- search popup -->
                            <!-- search popup -->
                        </div>
                    </div>

                    <!-- //search button -->
                </ul>
            </div>
            <!-- toggle switch for light and dark theme -->
